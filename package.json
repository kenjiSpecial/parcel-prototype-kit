{
  "name": "paracel-prtotype-kit",
  "version": "0.0.0",
  "description": "prototype tool with parcel and dan-shari-gl",
  "cjs": "build/tubu.cjs.js",
  "module": "build/tubu.esm.js",
  "main": "build/tubu.js",
  "libName": "Tubu",
  "license": "MIT",
  "scripts": {
    "build": "rollup -c",
    "00": "parcel examples/00/index.html --no-hmr --out-dir dev/00 --port 1234",
