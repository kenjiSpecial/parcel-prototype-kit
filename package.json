{
  "name": "paracel-prtotype-kit",
  "version": "1.0.0",
  "description": "prototype tool with parcel",
  "cjs": "build/tubu.cjs.js",
  "module": "build/tubu.esm.js",
  "main": "build/tubu.js",
  "libName": "Tubu",
  "license": "MIT",
  "scripts": {
    "build": "rollup -c",
    "00": "parcel examples/00-example/index.html --no-hmr",
    "01": "parcel examples/01-loadingExample/index.html --no-hmr",
    "dist00": "parcel build examples/00-example/index.html -d docs/00 --public-url ./",
    "dist01": "parcel build examples/01-loadingExample/index.html -d docs/01 --public-url ./",
    "screenshot00": "node ./node/screenshot.js --dir=00",
    "screenshot01": "node ./node/screenshot.js --dir=01",
    "docs": "python -m SimpleHTTPServer 8000"
  },
  "dependencies": {
    "tubugl-core": "^1.6.0",
    "wolfy87-eventemitter": "^5.2.5"
  },
  "devDependencies": {
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-preset-env": "^1.6.1",
    "babelrc-rollup": "^3.0.0",
    "glslify-bundle": "^5.0.0",
    "glslify-deps": "^1.3.1",
    "gsap": "^1.20.4",
    "minimist": "^1.2.0",
    "parcel-bundler": "^1.9.4",
    "parcel-plugin-json-url-loader": "^0.1.1",
    "puppeteer": "^1.5.0",
    "rollup": "^0.59.4",
    "rollup-plugin-babel": "^3.0.3",
    "rollup-plugin-commonjs": "^9.1.0",
    "rollup-plugin-node-resolve": "^3.3.0",
    "tubugl-helper": "^1.2.0"
  }
}
