{
	"name": "ts-parcel-as-lib-builder",
	"version": "0.0.0",
	"description": "Example project: Parcel as js Library builder",
	"main": "src/index.ts",
	"author": "satotake",
	"license": "MIT",
	"scripts": {
		"build": "parcel build src/index.ts --global FOUR",
		"doc": "typedoc --out docs --theme minimal --mode file src",
		"start": "parcel serve dev/index.html --no-hmr",
		"lint": "tslint -t stylish --project \"tsconfig.json\""
	},
	"dependencies": {
		"firebase": "^6.6.2",
		"vue": "^2.6.10",
		"vue-property-decorator": "^8.2.2"
	},
	"devDependencies": {
		"@vue/component-compiler-utils": "^3.0.0",
		"glslify-bundle": "^5.1.1",
		"glslify-deps": "^1.3.1",
		"parcel-bundler": "^1.12.3",
		"prettier": "^1.18.2",
		"tslint": "^5.20.0",
		"tslint-config-prettier": "^1.18.0",
		"tslint-microsoft-contrib": "^6.2.0",
		"tsutils": "^3.17.1",
		"typedoc": "^0.15.0",
		"typescript": "^3.6.3",
		"vue-template-compiler": "^2.6.10"
	}
}
